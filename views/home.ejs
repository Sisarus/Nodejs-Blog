<%- include("partials/header") -%>

<h1>Home page</h1>

<p><%= homePageText %></p>
<% posts.forEach((post)=>{ %>
<div>
  <h1><%= post.title %></h1>
  <p>
    <%= post.content.substring(0, 100) + " ..." %>
    <a href="/posts/<%= post.title %> ">Read More</a>
  </p>
</div>
<% }) %> <%- include("partials/footer") -%>
